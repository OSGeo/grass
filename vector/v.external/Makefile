MODULE_TOPDIR = ../..

PGM=v.external

LIBES = $(VECTORLIB) $(GISLIB) $(DBMIBASELIB) $(GPROJLIB) $(GDALLIBS) $(PQLIBPATH) $(PQLIB)
DEPENDENCIES = $(VECTORDEP) $(GISDEP) $(DBMIBASEDEP) $(GPROJDEP)

EXTRA_INC = $(VECT_INC) $(PROJINC)
