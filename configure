$as_echo_n "checking for MacOSX App... " >&6; }
case "$enable_macosx_app" in
	yes)	MACOSX_APP=1	;;
	no)	MACOSX_APP=	;;
	*)	as_fn_error $? "*** You must answer yes or no." "$LINENO" 5	;;
esac
