if test -n "$with_includes"; then
    for dir in $with_includes; do
        if test -d "$dir"; then
            INCLUDE_DIRS="$INCLUDE_DIRS -I$dir"
        else
            { echo "configure: error: *** Include directory $dir does not exist." 1>&2; exit 1; }
        fi
    done
    CPPFLAGS="$CPPFLAGS $INCLUDE_DIRS"
fi


