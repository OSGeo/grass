msgstr "Particionar pontos aleatoriamente em conjuntos de teste/treinamento."

#: ../gui/wxpython/menustrings.py:633 ../gui/wxpython/menustrings.py:1578
msgid "Tests for normality for vector points"
msgstr "Testes de normalidade para pontos vetoriais"

#: ../gui/wxpython/menustrings.py:634 ../gui/wxpython/menustrings.py:755
#: ../gui/wxpython/menustrings.py:1579 ../gui/wxpython/menustrings.py:1700
msgid "Tests for normality for vector points."
msgstr "Testa a normalidade dos pontos vetoriais."

#: ../gui/wxpython/menustrings.py:635 ../gui/wxpython/menustrings.py:1580
msgid "Lidar analysis"
msgstr "Análise Lidar"

#: ../gui/wxpython/menustrings.py:638 ../gui/wxpython/menustrings.py:1583
msgid "Detect edges"
msgstr "Detectar bordas"

#: ../gui/wxpython/menustrings.py:639 ../gui/wxpython/menustrings.py:1584
msgid "Detects the object's edges from a LIDAR data set."
msgstr "Detecta as bordas do objeto a partir de um conjunto de dados LIDAR."

#: ../gui/wxpython/menustrings.py:640 ../gui/wxpython/menustrings.py:1585
