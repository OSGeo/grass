#: ../lib/init/grass.py:212
msgid "WARNING"
msgstr "ПРЕДУПРЕЖДЕНИЕ"

#: ../lib/init/grass.py:216 ../lib/gis/parser.c:635
msgid "ERROR"
msgstr "ОШИБКА"

#: ../lib/init/grass.py:217 ../lib/init/grass.py:2646
msgid "Exiting..."
msgstr "Завершение работы…"

#: ../lib/init/grass.py:373
msgid "Usage"
msgstr "Использование"

#: ../lib/init/grass.py:367 ../lib/gis/parser_rest.c:146
#: ../lib/gis/parser_html.c:153
msgid "Flags"
msgstr "Флаги"

#: ../lib/init/grass.py:375
msgid "print this help message"
msgstr "вывести эту справку"

#: ../lib/init/grass.py:376
msgid "show version information and exit"
msgstr "вывести информацию о версии и выйти"

#: ../lib/init/grass.py:377
msgid "create given database, location or mapset if it doesn't exist"
msgstr "в случае отсутствия создать указанную базу данных, область или набор"

#: ../lib/init/grass.py:379
msgid "exit after creation of location or mapset. Only with -c flag"
msgstr "выйти после создания области или набора. Только с флагом -c"

#: ../lib/init/grass.py:382
msgid ""
"force removal of .gislock if exists (use with care!). Only with --text flag"
msgstr ""
"принудительно удалять .gislock при его наличии (использовать с "
"осторожностью!). Только с флагом --text"

#: ../lib/init/grass.py:385
msgid "use text based interface (skip graphical welcome screen)"
msgstr ""
"использовать текстовый интерфейс (пропустить графический экран приветствия)"

#: ../lib/init/grass.py:386 ../lib/init/grass.py:388 ../lib/init/grass.py:390
msgid "and set as default"
msgstr "и установить в качестве параметра по умолчанию"

#: ../lib/init/grass.py:387
msgid "use text based interface (show graphical welcome screen)"
msgstr ""
"использовать текстовый интерфейс (отображать графический экран приветствия)"

#: ../lib/init/grass.py:389
msgid "use $DEFAULT_GUI graphical user interface"
msgstr "использовать графический интерфейс $DEFAULT_GUI"

#: ../lib/init/grass.py:391
msgid "print GRASS configuration parameters"
msgstr "вывести параметры конфигурации GRASS"

#: ../lib/init/grass.py:393
msgid "options: arch,build,compiler,date,path,revision,svn_revision,version"
msgstr "параметры: arch,build,compiler,date,path,revision,svn_revision,version"

#: ../lib/init/grass.py:381 ../lib/gis/parser_rest.c:179
#: ../lib/gis/parser_html.c:201
msgid "Parameters"
msgstr "Параметры"

#: ../lib/init/grass.py:396
msgid "initial GRASS database directory"
msgstr "исходный каталог базы данных GRASS"

#: ../lib/init/grass.py:397
msgid "directory containing Locations"
msgstr "каталог для размещения областей"

#: ../lib/init/grass.py:398
msgid "initial GRASS Location"
msgstr "исходная область GRASS"

#: ../lib/init/grass.py:400
msgid ""
"directory containing Mapsets with one common coordinate system (projection)"
msgstr ""
"каталог для размещения наборов с единой общей системой координат (проекция)"

#: ../lib/init/grass.py:403
msgid "initial GRASS Mapset"
msgstr "исходный набор GRASS"

#: ../lib/init/grass.py:404
msgid "fully qualified initial Mapset directory"
msgstr "полное имя исходного каталога наборов"

#: ../lib/init/grass.py:405
msgid "Environment variables relevant for startup"
msgstr "Переменные окружения, имеющие отношение к запуску"

#: ../lib/init/grass.py:406
msgid "select GUI (text, gui, gtext)"
msgstr "выбор графического интерфейса (text, gui, gtext)"

#: ../lib/init/grass.py:407
msgid "set html web browser for help pages"
msgstr "задать браузер html-файлов для просмотра справки"

#: ../lib/init/grass.py:409
msgid "set additional path(s) to local GRASS modules or user scripts"
msgstr ""
"задать дополнительный путь (пути) к локальным модулям GRASS или "
"пользовательским сценариям"

#: ../lib/init/grass.py:412
msgid "set additional GISBASE for locally installed GRASS Addons"
msgstr ""
"задать дополнительный GISBASE для локально установленных расширений GRASS"

#: ../lib/init/grass.py:414
msgid "set Python interpreter name to override 'python'"
msgstr "задать имя интерпретатора Python для переопределения значения «python»"

#: ../lib/init/grass.py:415
msgid "execute GRASS module or script"
msgstr "запустить модуль или сценарий GRASS"

#: ../lib/init/grass.py:416
msgid "provided executable will be executed in GRASS session"
msgstr "данный исполняемый файл будет выполнен в сеансе GRASS"

#: ../lib/init/grass.py:417
msgid "GRASS module, script or any other executable"
msgstr "модуль, сценарий или любой другой исполняемый файл GRASS"

#: ../lib/init/grass.py:418
msgid "parameters of the executable"
msgstr "параметры исполняемого файла"

#: ../lib/init/grass.py:419
msgid "standard flags"
msgstr "стандартные флаги"

#: ../lib/init/grass.py:420
msgid "create temporary location (use with the --exec flag)"
msgstr "создать временную область (используется с флагом --exec)"

#: ../lib/init/grass.py:422
msgid "created in a temporary directory and deleted at exit"
msgstr "создаётся во временном каталоге и удаляется при выходе"

#: ../lib/init/grass.py:424
msgid "create temporary mapset (use with the --exec flag)"
msgstr "создать временный набор (используется с флагом --exec)"

#: ../lib/init/grass.py:426
msgid "created in the specified location and deleted at exit"
msgstr "создаётся в указанном расположении и удаляется при выходе"

#: ../lib/init/grass.py:452
msgid "The APPDATA variable is not set, ask your operating system support"
msgstr ""
"Переменная APPDATA не задана, обратитесь в службу поддержки операционной "
"системы"

#: ../lib/init/grass.py:459
msgid ""
"The APPDATA variable points to directory which does not exist, ask your "
"operating system support"
msgstr ""
"Переменная APPDATA указывает на несуществующий каталог, обратитесь в службу "
"поддержки операционной системы"

#: ../lib/init/grass.py:474
#, python-format
msgid "Failed to create configuration directory '%s' with error: %s"
msgstr "Сбой создания каталога конфигурации «%s» с ошибкой: %s"

#: ../lib/init/grass.py:483
#, python-format
msgid ""
"Unable to create temporary directory <grass7-%(user)s-%(lock)s>! Exiting."
msgstr ""
"Не удалось создать временный каталог <grass7-%(user)s-%(lock)s>! Завершение "
"работы."

#: ../lib/init/grass.py:576
#, python-brace-format
msgid "Invalid line in RC file ({file}): '{line}' ({error})\n"
msgstr "Недопустимая строка в файле RC ({file}): «{line}» ({error})\n"

#: ../lib/init/grass.py:583
#, python-brace-format
msgid "Empty RC file ({file})"
msgstr "Пустой файл RC ({file})"

#: ../lib/init/grass.py:682
#, python-format
msgid "GUI <%s> not supported in this version"
msgstr "Графический интерфейс <%s> не поддерживается в этой версии"

#: ../lib/init/grass.py:910
msgid ""
"The python command does not work as expected.\n"
"Please check your installation or set the GRASS_PYTHON environment variable."
msgstr ""
"Команда python работает неправильно.\n"
"Проверьте правильность установки или задайте переменную окружения "
"GRASS_PYTHON."

#: ../lib/init/grass.py:915
msgid "GRASS GUI not found. Please check your installation."
msgstr ""
"Не найден графический интерфейс GRASS. Проверьте правильность установки."

#: ../lib/init/grass.py:917
msgid ""
"{}\n"
"Switching to text based interface mode."
msgstr ""
"{}\n"
"Выполняется переключение на текстовый режим работы."

#: ../lib/init/grass.py:926
msgid ""
"It appears that the X Windows system is not active.\n"
"A graphical based user interface is not supported.\n"
"(DISPLAY variable is not set.)\n"
"Switching to text based interface mode."
msgstr ""
"Система X Windows не работает.\n"
"Графический интерфейс не поддерживается.\n"
"(переменная DISPLAY не установлена.)\n"
"Выполняется переключение на текстовый режим работы."

#: ../lib/init/grass.py:1001
#, python-brace-format
msgid ""
"Unable to create new location because the location <{location}> already "
"exists."
msgstr ""
"Не удалось создать новую область, так как область <{location}> уже "
"существует."

#: ../lib/init/grass.py:1007
#, python-brace-format
msgid "Unable to create new location <{location}> because <{path}> is a file."
msgstr ""
"Не удалось создать новую область <{location}>, потому что <{path}> является "
"файлом."

#: ../lib/init/grass.py:1011
#, python-brace-format
msgid ""
"Unable to create new location <{location}> because the directory <{path}> "
"already exists."
msgstr ""
"Не удалось создать новую область <{location}>, потому что каталог <{path}> "
"уже существует."

#: ../lib/init/grass.py:1017
#, python-brace-format
msgid ""
"Unable to create new location in the directory <{path}> for an unknown "
"reason."
msgstr ""
"Не удалось создать новую область в каталоге <{path}> по неизвестной причине."

#: ../lib/init/grass.py:1101
msgid ""
"Mapset <{}> already exists. Unable to create a new temporary mapset of that "
"name."
msgstr ""
"Набор <{}> уже существует. Не удалось создать новый временный наборс таким "
"именем."

#: ../lib/init/grass.py:1106
msgid "Mapset <{}> already exists."
msgstr "Набор <{}> уже существует."

#: ../lib/init/grass.py:1114
msgid "A new mapset can be created using '-c' flag."
msgstr "Новый набор может быть создан с помощью флага «-c»."

#: ../lib/init/grass.py:1116
msgid "Maybe you meant a different directory."
msgstr "Вероятно, имелся в виду другой каталог."

#: ../lib/init/grass.py:1145
msgid "Provide CRS to create a location"
msgstr "Для создания области необходимо предоставить CRS"

#: ../lib/init/grass.py:1147
msgid "Creating new GRASS GIS location <{}>..."
msgstr "Создание новой области GRASS GIS <{}>…"

#: ../lib/init/grass.py:1160
#, python-brace-format
msgid "Unable to create new mapset <{mapset}> because <{path}> is a file."
msgstr ""
"Не удалось создать новый набор <{mapset}>, потому что <{path}> является "
"файлом."

#: ../lib/init/grass.py:1169
msgid ""
"The mapset <{}> is missing the WIND file (computational region). It will be "
"fixed now. Note that this warning may become an error in future versions."
msgstr ""
"В наборе <{}> отсутствует файл WIND (область вычисления). Эта ошибка сейчас "
"будет исправлена. Имейте в виду, что данное предупреждение в будущих версиях "
"программы может перейти в разряд ошибки."

#: ../lib/init/grass.py:1179
msgid ""
"No CRS is needed for creating mapset. Did you mean to create a new location?"
msgstr ""
"Для создания набора не требуется CRS. Вероятно, предполагалось создать новую "
"область?"

#: ../lib/init/grass.py:1184
msgid "Creating new GRASS GIS mapset <{}>..."
msgstr "Создание нового набора GRASS GIS <{}>…"

#: ../lib/init/grass.py:1213
msgid ""
"GRASS GIS database directory, location and mapset not set properly. Use GUI "
"or command line to set them."
msgstr ""
"Каталог базы данных, область и набор GRASS GIS заданы неверно. Задайте их в "
"графическом интерфейсе или командной строке."

#: ../lib/init/grass.py:1091
#, python-format
msgid ""
"Invalid user interface specified - <%s>. Use the --help option to see valid "
"interface names."
msgstr ""
"Указан недопустимый интерфейс пользователя — <%s>. Используйте параметр --"
"helpдля вывода допустимых имён интерфейса."

#: ../lib/init/grass.py:1111
#, python-brace-format
msgid ""
"Error in GUI startup. See messages above (if any) and if necessary, please "
"report this error to the GRASS developers.\n"
"On systems with package manager, make sure you have the right GUI package, "
"probably named grass-gui, installed.\n"
"To run GRASS GIS in text mode use the --text flag.\n"
"Use '--help' for further options\n"
"     {cmd_name} --help\n"
"See also: https://grass.osgeo.org/{cmd_name}/manuals/helptext.html"
msgstr ""
"Ошибка при запуске графического интерфейса. Просмотрите сообщения выше (если "
"имеются) и при необходимости, сообщите об ошибке разработчикам GRASS.\n"
"На системах с менеджером пакетов необходимо убедиться в том, что установлен "
"правильный пакет графического интерфейса, вероятно с именем grass-gui.\n"
"Для запуска ГИС GRASS в текстовом режиме необходимо использовать флаг --text."
"\n"
"Также для получения информации о других параметрах можно использовать флаг --"
"help \n"
"     {cmd_name} --help\n"
"Смотрите также: https://grass.osgeo.org/{cmd_name}/manuals/helptext.html"

#: ../lib/init/grass.py:1123
msgid ""
"Exit was requested in GUI.\n"
"GRASS GIS will not start. Bye."
msgstr ""
"В графическом интерфейсе было инициировано завершение работы.\n"
"Запуск ГИС GRASS не состоится. До свиданья."

#: ../lib/init/grass.py:1126
msgid ""
"Invalid return code from GUI startup script.\n"
"Please advise GRASS developers of this error."
msgstr ""
"Недопустимый код возврата от сценария запуска графического интерфейса.\n"
"Сообщите об этой ошибке разработчикам GRASS."

#: ../lib/init/grass.py:1273
#, python-brace-format
msgid ""
"Error reading data path information from g.gisenv.\n"
"GISDBASE={gisdbase}\n"
"LOCATION_NAME={location}\n"
"MAPSET={mapset}\n"
"\n"
"Check the <{file}> file."
msgstr ""
"Ошибка чтения информации о пути к данным из g.gisenv.\n"
"GISDBASE={gisdbase}\n"
"LOCATION_NAME={location}\n"
"MAPSET={mapset}\n"
"\n"
"Проверьте файл <{file}>."

#: ../lib/init/grass.py:1572
#, python-format
msgid "Path '%s' doesn't exist"
msgstr "Путь «%s» не существует"

#: ../lib/init/grass.py:1574
#, python-format
msgid "Path '%s' not accessible."
msgstr "Путь «%s» недоступен."

#: ../lib/init/grass.py:1581
#, python-format
msgid "You are not the owner of '%s'."
msgstr "Данный пользователь не является владельцем «%s»."

#: ../lib/init/grass.py:1591
#, python-format
msgid ""
"%(user)s is currently running GRASS in selected mapset (file %(file)s "
"found). Concurrent use not allowed.\n"
"You can force launching GRASS using -f flag (note that you need permission "
"for this operation). Have another look in the processor manager just to be "
"sure..."
msgstr ""
"%(user)s в данный момент использует GRASS для выбранного набора  (найден "
"файл %(file)s). Одновременное использование не допускается.\n"
"Можно принудительно запустить GRASS с флагом -f (обратите внимание на то,  "
"что для проведения этой операции необходимо соответствующее разрешение).  "
"Для уверенности следите за работой программы в диспетчер задач…"

#: ../lib/init/grass.py:1602
#, python-format
msgid ""
"%(user)s is currently running GRASS in selected mapset (file %(file)s "
"found). Forcing to launch GRASS..."
msgstr ""
"%(user)s в данный момент использует GRASS в выбранном наборе (найден файл "
"%(file)s). Выполняется принудительный запуск GRASS…"

#: ../lib/init/grass.py:1609
#, python-format
msgid ""
"Unable to properly access '%s'.\n"
"Please notify system personnel."
msgstr ""
"Не удалось получить доступ к «%s».\n"
"Поставьте в известность обслуживающий персонал."

#: ../lib/init/grass.py:1638
msgid "Building user fontcap..."
msgstr "Построение файла fontcap пользователя…"

#: ../lib/init/grass.py:1707
msgid "The SHELL variable is not set"
msgstr "Переменная SHELL не установлена"

#: ../lib/init/grass.py:1724
#, python-brace-format
msgid "Unsupported shell <{sh}>: {env_file}"
msgstr "Командная оболочка <{sh}> не поддерживается: {env_file}"

#: ../lib/init/grass.py:1742
#, python-format
msgid "Executing <%s> ..."
msgstr "Выполнение <%s> …"

#: ../lib/init/grass.py:1768
#, python-brace-format
msgid ""
"Execution of <{cmd}> failed:\n"
"{error}"
msgstr ""
"Сбой выполнения <{cmd}>:\n"
"{error}"

#: ../lib/init/grass.py:1782
#, python-format
msgid "Execution of <%s> finished."
msgstr "Выполнение <%s> завершено."

#: ../lib/init/grass.py:1816
#, python-brace-format
msgid "Unable to close GUI. {0}"
msgstr "Не удалось закрыть графический интерфейс. {0}"

#: ../lib/init/grass.py:1835
#, python-format
msgid "Welcome to GRASS GIS %s"
msgstr "Добро пожаловать в GRASS GIS %s"

#: ../lib/init/grass.py:1862
msgid "GRASS GIS homepage:"
msgstr "Домашняя страница GRASS GIS:"

#. GTC Running through: SHELL NAME
#: ../lib/init/grass.py:1864
msgid "This version running through:"
msgstr "Эта версия работает в командной оболочке:"

#: ../lib/init/grass.py:1867
msgid "Help is available with the command:"
msgstr "Вывод справки:"

#: ../lib/init/grass.py:1868
msgid "See the licence terms with:"
msgstr "Вывод условий лицензии:"

#: ../lib/init/grass.py:1869
msgid "See citation options with:"
msgstr "Вывод параметров ссылки:"

#: ../lib/init/grass.py:1874
msgid "If required, restart the GUI with:"
msgstr "При необходимости перезагрузки графического интерфейса:"

#: ../lib/init/grass.py:1876
msgid "Start the GUI with:"
msgstr "Запуск графического интерфейса:"

#: ../lib/init/grass.py:1878
msgid "When ready to quit enter:"
msgstr "Завершение работы:"

#: ../lib/init/grass.py:2044
msgid "2D and 3D raster MASKs present"
msgstr "Имеются двух- и трёхмерные растровые маски"

#: ../lib/init/grass.py:2045
msgid "Raster MASK present"
msgstr "Имеется растровая маска"

#: ../lib/init/grass.py:2046
msgid "3D raster MASK present"
msgstr "Имеется трёхмерная растровая маска"

#: ../lib/init/grass.py:2098
msgid "Done."
msgstr "Готово."

#: ../lib/init/grass.py:2100
msgid "Goodbye from GRASS GIS"
msgstr "До новых встреч с GRASS GIS"

#: ../lib/init/grass.py:2105
msgid "Cleaning up temporary files..."
msgstr "Выполняется удаление временных файлов…"

#: ../lib/init/grass.py:2157
msgid "Please install the GRASS GIS development package"
msgstr "Установите пакет для разработки GRASS GIS"

#: ../lib/init/grass.py:2206
#, python-format
msgid "Parameter <%s> not supported"
msgstr "Параметр <%s> не поддерживается"

#: ../lib/init/grass.py:2255
#, python-format
msgid "use mapset %(metavar)s"
msgstr "используйте набор %(metavar)s"

#: ../lib/init/grass.py:2261
#, python-format
msgid "use temporary mapset in location %(metavar)s"
msgstr "используйте временный набор в области %(metavar)s"

#: ../lib/init/grass.py:2269
msgid "path to mapset (or location if creating one)"
msgstr "путь к набору (или области при её создании)"

#: ../lib/init/grass.py:2272
msgid "use temporary mapset"
msgstr "используйте временный набор"

#: ../lib/init/grass.py:2279
#, python-format
msgid "use temporary location with %(metavar)s (EPSG, georeferenced file, ...)"
msgstr ""
"используйте временную область с %(metavar)s (EPSG, файл географической "
"привязки, …)"

#: ../lib/init/grass.py:2286
msgid "remove lock if present"
msgstr "снять блокировку при её наличии"

#: ../lib/init/grass.py:2335
msgid "execute module or script (followed by executable with arguments)"
msgstr ""
"выполнить модуль или сценарий (следует за исполняемым файлом с аргументами)"

#: ../lib/init/grass.py:2391
msgid "Flag -e requires also flag -c"
msgstr "Флаг -e требует наличия флага -c"

#: ../lib/init/grass.py:2393
msgid "Flag -c requires name of location or mapset"
msgstr "Флаг -c требует имени области или набора"

#: ../lib/init/grass.py:2396
