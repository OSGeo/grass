#: ../gui/wxpython/menustrings.py:119 ../gui/wxpython/menustrings.py:1159
msgid "MPEG-1 export"
msgstr "Экспорт MPEG-1"

#: ../gui/wxpython/menustrings.py:120 ../gui/wxpython/menustrings.py:1160
msgid "Converts raster map series to MPEG movie."
msgstr "Конвертирует последовательности растровой карты в видеофайл MPEG."

#: ../gui/wxpython/menustrings.py:121 ../gui/wxpython/menustrings.py:1161
msgid "PNG export"
msgstr "Экспорт PNG"

#: ../gui/wxpython/menustrings.py:122 ../gui/wxpython/menustrings.py:1162
msgid "Export a GRASS raster map as a non-georeferenced PNG image."
msgstr "Экспорт растровой карты GRASS в виде изображения PNG без геопривязки."

#: ../gui/wxpython/menustrings.py:123 ../gui/wxpython/menustrings.py:1163
msgid "PPM export"
msgstr "Экспорт PPM"

#: ../gui/wxpython/menustrings.py:124 ../gui/wxpython/menustrings.py:1164
msgid "Converts a GRASS raster map to a PPM image file."
msgstr "Конвертирует растровую карту GRASS в файл изображения PPM."

#: ../gui/wxpython/menustrings.py:125 ../gui/wxpython/menustrings.py:1165
msgid "PPM from RGB export"
msgstr "Экспорт PPM из RGB"

#: ../gui/wxpython/menustrings.py:126 ../gui/wxpython/menustrings.py:1166
msgid "Converts 3 GRASS raster layers (R,G,B) to a PPM image file."
msgstr ""
"Конвертирует трёхмерные растровые слои GRASS (R,G,B) в файл изображения PPM."

#: ../gui/wxpython/menustrings.py:127 ../gui/wxpython/menustrings.py:144
#: ../gui/wxpython/menustrings.py:1167 ../gui/wxpython/menustrings.py:1184
msgid "POV-Ray export"
msgstr "Экспорт POV-Ray"

#: ../gui/wxpython/menustrings.py:128 ../gui/wxpython/menustrings.py:1168
msgid "Converts a raster map layer into a height-field file for POV-Ray."
msgstr "Конвертирует растровый слой карты в файл height-field для POV-Ray."

#: ../gui/wxpython/menustrings.py:129 ../gui/wxpython/menustrings.py:1169
msgid "VRML export"
msgstr "Экспорт VRML"

#: ../gui/wxpython/menustrings.py:130 ../gui/wxpython/menustrings.py:1170
msgid "Exports a raster map to the Virtual Reality Modeling Language (VRML)."
msgstr "Экспорт растровой карты в VRML (Virtual Reality Modeling Language)."

#: ../gui/wxpython/menustrings.py:131 ../gui/wxpython/menustrings.py:148
#: ../gui/wxpython/menustrings.py:159 ../gui/wxpython/menustrings.py:1171
#: ../gui/wxpython/menustrings.py:1188 ../gui/wxpython/menustrings.py:1199
msgid "VTK export"
msgstr "Экспорт VTK"

#: ../gui/wxpython/menustrings.py:132 ../gui/wxpython/menustrings.py:1172
msgid "Converts raster maps into the VTK-ASCII format."
msgstr "Конвертирует растровые карты в формат VTK-ASCII."

#: ../gui/wxpython/menustrings.py:133 ../gui/wxpython/menustrings.py:1173
msgid "Pack raster map"
msgstr "Упаковка растровой карты"

#: ../gui/wxpython/menustrings.py:134 ../gui/wxpython/menustrings.py:1174
msgid "Exports a raster map as GRASS GIS specific archive file"
msgstr "Экспорт растровой карты в специализированный архивный файл ГИС GRASS"

#: ../gui/wxpython/menustrings.py:135 ../gui/wxpython/menustrings.py:1175
msgid "Export vector map"
msgstr "Экспорт векторной карты"

#: ../gui/wxpython/menustrings.py:137 ../gui/wxpython/menustrings.py:1177
msgid ""
"Exports a vector map layer to any of the supported OGR vector formats. By "
"default a vector map layer is exported to OGC GeoPackage format."
msgstr ""
"Экспорт векторного слоя карты в любой поддерживаемый векторный формат OGR. "
"По умолчанию слой векторной карты экспортируется в формат GeoPackage OGC."

#: ../gui/wxpython/menustrings.py:138 ../gui/wxpython/menustrings.py:1178
msgid "ASCII points or GRASS ASCII vector export"
msgstr "Экспорт ASCII-точек или ASCII-вектора GRASS"

#: ../gui/wxpython/menustrings.py:139 ../gui/wxpython/menustrings.py:1179
msgid ""
"Exports a vector map to a GRASS ASCII vector representation. By default only "
"features with category are exported. To export all features use 'layer=-1'."
msgstr ""
"Экспорт векторной карты в виде ASCII-вектора GRASS. По умолчанию экспорт "
"производится только для объектов с указанной категорией. Для экспорта всех "
"объектов необходимо использовать параметр «layer=-1»."

#: ../gui/wxpython/menustrings.py:140 ../gui/wxpython/menustrings.py:1180
msgid "DXF export"
msgstr "Экспорт DXF"

#: ../gui/wxpython/menustrings.py:141 ../gui/wxpython/menustrings.py:1181
msgid "Exports vector map to DXF file format."
msgstr "Экспорт векторной карты в файл формата DXF."

#: ../gui/wxpython/menustrings.py:142 ../gui/wxpython/menustrings.py:1182
msgid "PostGIS export"
msgstr "Экспорт PostGIS"

#: ../gui/wxpython/menustrings.py:143 ../gui/wxpython/menustrings.py:1183
msgid "Exports a vector map layer to PostGIS feature table."
msgstr "Экспорт слоя векторной карты в таблицу объектов PostGIS."

#: ../gui/wxpython/menustrings.py:145 ../gui/wxpython/menustrings.py:1185
msgid "Converts GRASS x,y,z points to POV-Ray x,z,y format."
msgstr "Конвертирует точки x,y,z GRASS в формат x,z,y POV-Ray."

#: ../gui/wxpython/menustrings.py:146 ../gui/wxpython/menustrings.py:1186
msgid "SVG export"
msgstr "Экспорт SVG"

#: ../gui/wxpython/menustrings.py:147 ../gui/wxpython/menustrings.py:1187
msgid "Exports a vector map to SVG file."
msgstr "Экспорт векторной карты в файл SVG."

#: ../gui/wxpython/menustrings.py:149 ../gui/wxpython/menustrings.py:1189
msgid "Converts a vector map to VTK ASCII output."
msgstr "Конвертирует векторную карту в вывод ASCII VTK."

#: ../gui/wxpython/menustrings.py:150 ../gui/wxpython/menustrings.py:1190
msgid "Pack vector map"
msgstr "Упаковка векторной карты"

#: ../gui/wxpython/menustrings.py:151 ../gui/wxpython/menustrings.py:1191
msgid "Exports a vector map as GRASS GIS specific archive file"
msgstr "Экспорт векторной карты в специализированный архивный файла ГИС GRASS"

#: ../gui/wxpython/menustrings.py:152 ../gui/wxpython/menustrings.py:1192
msgid "Export 3D raster maps"
msgstr "Экспорт трёхмерных растровых карт"

#: ../gui/wxpython/menustrings.py:153 ../gui/wxpython/menustrings.py:1193
msgid "ASCII 3D export"
msgstr "Экспорт трёхмерного слоя в ASCII-текст"

#: ../gui/wxpython/menustrings.py:154 ../gui/wxpython/menustrings.py:1194
msgid "Converts a 3D raster map layer into a ASCII text file."
msgstr "Конвертирует трёхмерный растровый слой карты в текстовый ASCII-файл."

#: ../gui/wxpython/menustrings.py:155 ../gui/wxpython/menustrings.py:1195
msgid "Raw binary array 3D export"
msgstr "Экспорт необработанного двоичного трёхмерного массива"

#: ../gui/wxpython/menustrings.py:156 ../gui/wxpython/menustrings.py:1196
msgid "Exports a GRASS 3D raster map to a binary array."
msgstr "Экспорт трёхмерной растровой карты GRASS в двоичный массив."

#: ../gui/wxpython/menustrings.py:157 ../gui/wxpython/menustrings.py:1197
msgid "Vis5D export"
msgstr "Экспорт Vis5D"

#: ../gui/wxpython/menustrings.py:158 ../gui/wxpython/menustrings.py:1198
msgid "Exports GRASS 3D raster map to 3-dimensional Vis5D file."
msgstr "Экспорт трёхмерной растровой карты GRASS в трёхмерный файл Vis5D."

#: ../gui/wxpython/menustrings.py:160 ../gui/wxpython/menustrings.py:1200
msgid "Converts 3D raster maps into the VTK-ASCII format."
msgstr "Конвертирует трёхмерные растровые карты в формат VTK-ASCII."

#: ../gui/wxpython/menustrings.py:161 ../gui/wxpython/menustrings.py:1201
msgid "Export database table"
msgstr "Экспорт таблицы базы данных"

#: ../gui/wxpython/menustrings.py:163 ../gui/wxpython/menustrings.py:1203
msgid "Exports attribute tables into various formats."
msgstr "Экспорт таблицы атрибутов в различные форматы."

#: ../gui/wxpython/menustrings.py:165 ../gui/wxpython/menustrings.py:1205
#: ../gui/wxpython/modules/import_export.py:414
msgid "Link external raster data"
msgstr "Внешние растровые данные связи"

#: ../gui/wxpython/menustrings.py:166 ../gui/wxpython/menustrings.py:1206
msgid "Link GDAL supported raster data as a pseudo GRASS raster map layer."
msgstr ""
"Создание связи с поддерживаемыми GDAL растровыми данными как с псевдо-"
"растровым слоем карты GRASS."

#: ../gui/wxpython/menustrings.py:167 ../gui/wxpython/menustrings.py:1207
#: ../gui/wxpython/modules/import_export.py:576
msgid "Link external vector data"
msgstr "Связать внешние векторные данные"

#: ../gui/wxpython/menustrings.py:168 ../gui/wxpython/menustrings.py:1208
msgid "Creates a new pseudo-vector map as a link to an OGR-supported layer."
msgstr ""
"Создаёт новую псевдо-векторную карту в виде связи со слоем, поддерживаемым "
"OGR."

#: ../gui/wxpython/menustrings.py:169 ../gui/wxpython/menustrings.py:1209
msgid "Output format for raster data"
msgstr "Формат вывода для растровых данных"

#: ../gui/wxpython/menustrings.py:170 ../gui/wxpython/menustrings.py:1210
msgid ""
"Redirects raster output to file utilizing GDAL library rather than storing "
"in GRASS raster format."
msgstr ""
"Перенаправляет растровый вывод в файл с применением библиотеки GDAL, вместо "
"хранения в растровом формате GRASS."

#: ../gui/wxpython/menustrings.py:171 ../gui/wxpython/menustrings.py:1211
msgid "Output format for vector data"
msgstr "Формат вывода для векторных данных"

#: ../gui/wxpython/menustrings.py:172 ../gui/wxpython/menustrings.py:1212
msgid "Defines vector output format utilizing OGR library."
msgstr "Определяет векторный формат вывода с применением библиотеки OGR."

#: ../gui/wxpython/menustrings.py:173 ../gui/wxpython/menustrings.py:1213
msgid "Manage maps"
msgstr "Управление картами"

#: ../gui/wxpython/menustrings.py:174 ../gui/wxpython/menustrings.py:1214
#: ../gui/wxpython/gui_core/forms.py:630
#: ../gui/wxpython/modules/mcalc_builder.py:176
msgid "Copy"
msgstr "Копировать"

#: ../gui/wxpython/menustrings.py:175 ../gui/wxpython/menustrings.py:1215
msgid ""
"Creates copies of maps and other elements Copies available data files in the "
"current mapset search path to the user's current mapset."
msgstr ""
"Создаёт копии карт и других элементов. Копирует файлы данных, находящиеся в "
"пути поиска текущего набора, в текущий набор пользователя."

#: ../gui/wxpython/menustrings.py:176 ../gui/wxpython/menustrings.py:982
#: ../gui/wxpython/menustrings.py:1216 ../gui/wxpython/menustrings.py:1947
msgid "List"
msgstr "Список"

#: ../gui/wxpython/menustrings.py:177 ../gui/wxpython/menustrings.py:1217
msgid ""
"Lists available GRASS data base files of the user-specified data type "
"optionally using the search pattern."
msgstr ""
"Выводит список доступных файлов базы данных GRASS, относящихся к указанному "
"пользователем типу данных, с возможностью использования шаблона поиска."

#: ../gui/wxpython/menustrings.py:178 ../gui/wxpython/menustrings.py:970
#: ../gui/wxpython/menustrings.py:1218 ../gui/wxpython/menustrings.py:1935
#: ../gui/wxpython/rlisetup/frame.py:163 ../gui/wxpython/lmgr/layertree.py:457
msgid "Rename"
msgstr "Переименовать"

#: ../gui/wxpython/menustrings.py:179 ../gui/wxpython/menustrings.py:1219
msgid "Renames data base element files in the user's current mapset."
msgstr ""
"Переименовывает файлы элементов базы данных в текущем наборе пользователя."

#: ../gui/wxpython/menustrings.py:181 ../gui/wxpython/menustrings.py:1221
msgid ""
"Removes data base element files from the user's current mapset using the "
"search pattern."
msgstr ""
"Удаляет файлы элементов базы данных из текущего набора пользователя с "
"помощью шаблона поиска."

#: ../gui/wxpython/menustrings.py:182 ../gui/wxpython/menustrings.py:320
#: ../gui/wxpython/menustrings.py:623 ../gui/wxpython/menustrings.py:883
#: ../gui/wxpython/menustrings.py:1285 ../gui/wxpython/menustrings.py:1588
#: ../gui/wxpython/menustrings.py:1848
msgid "Map type conversions"
msgstr "Преобразования типа карты"

#: ../gui/wxpython/menustrings.py:183 ../gui/wxpython/menustrings.py:321
#: ../gui/wxpython/menustrings.py:1286
msgid "Raster to vector"
msgstr "Растр в вектор"

#: ../gui/wxpython/menustrings.py:184 ../gui/wxpython/menustrings.py:322
#: ../gui/wxpython/menustrings.py:1287
msgid "Converts a raster map into a vector map."
msgstr "Конвертирует растровую карту в векторную карту"

#: ../gui/wxpython/menustrings.py:185 ../gui/wxpython/menustrings.py:323
#: ../gui/wxpython/menustrings.py:1288
msgid "Raster series to 3D raster"
msgstr "Растровая последовательность в трёхмерный растр"

#: ../gui/wxpython/menustrings.py:186 ../gui/wxpython/menustrings.py:324
#: ../gui/wxpython/menustrings.py:1289
msgid "Converts 2D raster map slices to one 3D raster volume map."
msgstr ""
"Конвертирует срезы двухмерной растровой карты в одну растровую трёхмерную "
"объёмную карту."

#: ../gui/wxpython/menustrings.py:187 ../gui/wxpython/menustrings.py:325
#: ../gui/wxpython/menustrings.py:1290
msgid "Raster 2.5D to 3D raster"
msgstr "Растр 2.5D в растр 3D"

#: ../gui/wxpython/menustrings.py:188 ../gui/wxpython/menustrings.py:326
#: ../gui/wxpython/menustrings.py:1291
msgid "Creates a 3D volume map based on 2D elevation and value raster maps."
msgstr ""
"Создаёт трёхмерную объёмную карту из двухмерной карты высот и растровых карт "
"со значениями."

#: ../gui/wxpython/menustrings.py:189 ../gui/wxpython/menustrings.py:624
#: ../gui/wxpython/menustrings.py:1589
msgid "Vector to raster"
msgstr "Вектор в растр"

#: ../gui/wxpython/menustrings.py:190 ../gui/wxpython/menustrings.py:625
#: ../gui/wxpython/menustrings.py:1590
msgid "Converts (rasterize) a vector map into a raster map."
msgstr "Конвертирует (растеризует) векторную карту в растровую карту."

#: ../gui/wxpython/menustrings.py:191 ../gui/wxpython/menustrings.py:626
#: ../gui/wxpython/menustrings.py:1591
msgid "Vector to 3D raster"
msgstr "Вектор в трёхмерный растр"

#: ../gui/wxpython/menustrings.py:192 ../gui/wxpython/menustrings.py:627
#: ../gui/wxpython/menustrings.py:1592
msgid "Converts a vector map (only points) into a 3D raster map."
msgstr ""
"Конвертирует векторную карту (только точки) в трёхмерную растровую карту."

#: ../gui/wxpython/menustrings.py:193 ../gui/wxpython/menustrings.py:628
#: ../gui/wxpython/menustrings.py:1593
msgid "2D vector to 3D vector"
msgstr "2D вектор в 3D вектор"

#: ../gui/wxpython/menustrings.py:194 ../gui/wxpython/menustrings.py:629
#: ../gui/wxpython/menustrings.py:1594
msgid "Performs transformation of 2D vector features to 3D."
msgstr "Выполняет преобразование двухмерных векторных объектов в трёхмерные."

#: ../gui/wxpython/menustrings.py:195
msgid "3D raster to raster series"
msgstr "3D растр в растровую последовательность"

#: ../gui/wxpython/menustrings.py:196 ../gui/wxpython/menustrings.py:885
#: ../gui/wxpython/menustrings.py:1850
msgid "Converts 3D raster maps to 2D raster maps"
msgstr "Конвертирует трёхмерные растровые карты в двухмерные растровые карты"

#: ../gui/wxpython/menustrings.py:198 ../gui/wxpython/menustrings.py:2039
msgid "Manage Ground Control Points for Georectification"
msgstr "Управление наземными контрольными точками для геопривязки"

#: ../gui/wxpython/menustrings.py:199 ../gui/wxpython/menustrings.py:2040
msgid "Graphical modeler"
msgstr "Средство графического моделирования"

#: ../gui/wxpython/menustrings.py:200 ../gui/wxpython/menustrings.py:2041
msgid "Launch Graphical modeler"
msgstr "Запуск средства графического моделирования"

#: ../gui/wxpython/menustrings.py:202
msgid "Run model prepared by Graphical modeler"
msgstr "Запуск модели, подготовленной средством графического моделирования"

#: ../gui/wxpython/menustrings.py:203
msgid "3D image rendering"
msgstr "Отрисовка трёхмерного изображения"

#: ../gui/wxpython/menustrings.py:204
msgid ""
"Creates a 3D rendering of GIS data. Renders surfaces (raster data), 2D/3D "
"vector data, and volumes (3D raster data) in 3D."
msgstr ""
"Создаёт отрисовку трёхмерных данных ГИС. Отрисовку поверхностей (растровых "
"данных), двухмерных или трёхмерных векторных данных, а также объёмов "
"(трёхмерных растровых данных) в трёхмерном отображении."

#: ../gui/wxpython/menustrings.py:205 ../gui/wxpython/menustrings.py:1056
#: ../gui/wxpython/menustrings.py:2021 ../gui/wxpython/menustrings.py:2028
msgid "Animation tool"
msgstr "Инструмент анимации"

#: ../gui/wxpython/menustrings.py:206 ../gui/wxpython/menustrings.py:1057
#: ../gui/wxpython/menustrings.py:2022 ../gui/wxpython/menustrings.py:2029
msgid "Launch animation tool."
msgstr "Запуск инструмента анимации."

#: ../gui/wxpython/menustrings.py:207
msgid "Bearing/distance to coordinates"
msgstr "Ориентир/расстояние по координатам"

#: ../gui/wxpython/menustrings.py:208
msgid ""
"A simple utility for converting bearing and distance measurements to "
"coordinates and vice versa. It assumes a cartesian coordinate system"
msgstr ""
"Простая утилита для конвертирования измерений ориентирного направления и "
"расстояния в координаты и наоборот. Использует декартову систему координат"

#: ../gui/wxpython/menustrings.py:209 ../gui/wxpython/menustrings.py:2032
#: ../gui/wxpython/lmgr/toolbars.py:210 ../gui/wxpython/psmap/frame.py:55
msgid "Cartographic Composer"
msgstr "Компоновщик карт"

#: ../gui/wxpython/menustrings.py:210 ../gui/wxpython/menustrings.py:2033
msgid "Launch Cartographic Composer"
msgstr "Запуск компоновщика карт"

#: ../gui/wxpython/menustrings.py:212 ../gui/wxpython/menustrings.py:2047
msgid "Launch Map Swipe"
msgstr "Запуск Map Swipe"

#: ../gui/wxpython/menustrings.py:213
msgid "Launch script"
msgstr "Запуск сценария"

#: ../gui/wxpython/menustrings.py:214
msgid "Launches script file."
msgstr "Запуск файла сценария."

#: ../gui/wxpython/menustrings.py:215 ../gui/wxpython/gui_core/pyedit.py:743
msgid "Simple Python Editor"
msgstr "Простой редактор Python"

#: ../gui/wxpython/menustrings.py:216
msgid "Launches Simple Python Editor."
msgstr "Запуск простого редактора языка Python."

#: ../gui/wxpython/menustrings.py:217 ../gui/wxpython/gui_core/dialogs.py:2390
msgid "Close GUI"
msgstr "Закрыть графический интерфейс"

#: ../gui/wxpython/menustrings.py:218
msgid "Close graphical user interface."
msgstr "Выход из графического интерфейса пользователя."

#: ../gui/wxpython/menustrings.py:219 ../gui/wxpython/gui_core/dialogs.py:2360
#: ../gui/wxpython/gui_core/dialogs.py:2393
msgid "Quit GRASS GIS"
msgstr "Выйти из ГИС GRASS"

#: ../gui/wxpython/menustrings.py:220
msgid "Close GUI and exit GRASS GIS shell."
msgstr ""
"Выход из графического интерфейса пользователя и из командной оболочки ГИС "
"GRASS."

#: ../gui/wxpython/menustrings.py:221 ../gui/wxpython/menustrings.py:2077
msgid "&Settings"
msgstr "&Параметры"

#: ../gui/wxpython/menustrings.py:222 ../gui/wxpython/menustrings.py:1222
#: ../gui/wxpython/web_services/dialogs.py:882
#: ../gui/wxpython/mapdisp/statusbar.py:960
msgid "Computational region"
msgstr "Расчётный фрагмент"

#: ../gui/wxpython/menustrings.py:223 ../gui/wxpython/menustrings.py:1223
msgid "Show current region"
msgstr "Вывод текущего фрагмента"

#: ../gui/wxpython/menustrings.py:224 ../gui/wxpython/menustrings.py:226
#: ../gui/wxpython/menustrings.py:1224 ../gui/wxpython/menustrings.py:1226
msgid "Shows the extent and resolution of the computational region."
msgstr "Вывод охвата и разрешения расчётного фрагмента."

#: ../gui/wxpython/menustrings.py:225 ../gui/wxpython/menustrings.py:1225
msgid "Show region using latitude and longitude"
msgstr "Вывод фрагмента с указанием широты и долготы"

#: ../gui/wxpython/menustrings.py:227 ../gui/wxpython/menustrings.py:1227
msgid "Show current region extent in Map Display"
msgstr "Вывод текущего охвата фрагмента в отображении карты"

#: ../gui/wxpython/menustrings.py:228 ../gui/wxpython/menustrings.py:1228
msgid "Shows the extent of the computational region in Map Display."
msgstr "Вывод охвата расчётного фрагмента в отображении карты."

